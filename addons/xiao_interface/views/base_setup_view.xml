<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_interface_configuration" model="ir.ui.view">
            <field name="name">view.interface.config.settings.form</field>
            <field name="model">interface.config.settings</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <button string="确认" type="object" name="execute" class="oe_highlight"/>
                        or
                        <button string="取消" type="object" name="cancel" class="oe_link"/>
                    </header>
                    <group string="产品同步">
                        <field name="product_type_default"/>
                        <field name="product_type_parent_default"/>
                    </group>
                    <group string="用户同步">
                        <button name="button_sync_partner" type="object" string="立刻同步"/>
                        <newline/>
                        <field name="partner_company_default"/>
                        <field name="partner_warehouse_default"/>
                    </group>
                    <group string="订单同步">
                        <button name="button_sync_order" type="object" string="立刻同步"/>
                        <newline/>
                        <field name="order_tax_default"/>
                        <field name="order_delivery_default"/>
                        <field name="order_user_default"/>
                    </group>
                    <group string="货运同步">
                        <field name="delivery_company_code"/>
                    </group>
                    <group string="付款信息同步">

                        <button name="button_sync_payment" type="object" string="立刻同步"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_interface_sync_log_tree" model="ir.ui.view">
            <field name="name">view.interface.sync.log.tree</field>
            <field name="model">interface.sync.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="log_datetime"/>
                    <field name="model"/>
                    <field name="type"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>