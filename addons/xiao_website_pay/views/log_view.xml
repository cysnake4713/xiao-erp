<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_xiao_website_pay_log_tree" model="ir.ui.view">
            <field name="name">view.xiao.website.pay.log.tree</field>
            <field name="model">xiao.website.pay.log</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name" readonly="1"/>
                    <field name="state" readonly="1"/>
                    <field name="tianv_id" readonly="1"/>
                    <field name="pay_datetime" readonly="1"/>
                    <field name="price" readonly="1"/>
                    <field name="related_order_ids" readonly="1" widget="many2many_tags"/>
                    <field name="closed_order_ids" readonly="1" widget="many2many_tags"/>
                    <field name="is_reconciled"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>